{
  "name": "svelte-hot-app",
  "version": "1.0.0",
  "devDependencies": {
    "nollup": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-static-files": "^0.0.1",
    "rollup-plugin-svelte-hot": "^0.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev:rollup": "run-p start:dev autobuild",
    "dev": "NOLLUP=1 nollup -c",
    "start": "sirv dist --single",
    "start:dev": "sirv dist --single --dev"
  }
}
