{
  "name": "svelte-template-hot",
  "version": "1.0.0",
  "dependencies": {
    "sirv-cli": "^0.4.4",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "nollup": "^0.17.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte-hot": "^0.14.1",
    "rollup-plugin-terser": "^5.1.2",
    "rollup": "^1.26.3",
    "svelte": "^3.38.3"
  },
  "scripts": {
    "build": "rollup -c",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "cross-env LIVERELOAD=1 rollup -c -w",
    "dev:nollup": "nollup -c --hot --port 5000 --content-base ./public",
    "dev": "cross-env MUTE_NOLLUP=0 npm run dev:nollup",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev"
  }
}
